{
  "name": "@astrojs/deno",
  "description": "Deploy your Astro site to a Deno server",
  "version": "5.0.1",
  "type": "module",
  "author": "denoland",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/denoland/deno-astro-adapter.git"
  },
  "keywords": [
    "withastro",
    "astro-adapter"
  ],
  "bugs": "https://github.com/denoland/deno-astro-adapter/issues",
  "homepage": "https://github.com/denoland/deno-astro-adapter/",
  "exports": {
    ".": "./src/index.ts",
    "./server.ts": "./src/server.ts",
    "./__deno_imports.ts": "./src/__deno_imports.ts",
    "./package.json": "./package.json"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "test": "deno test --allow-run --allow-env --allow-read --allow-net --allow-ffi --allow-sys ./test/",
    "format": "deno fmt",
    "format:check": "deno fmt --check",
    "lint": "deno lint"
  },
  "dependencies": {
    "esbuild": "^0.20.2"
  },
  "peerDependencies": {
    "astro": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "astro": "^4.5.17",
    "typescript": "^5.4.4"
  }
}
